{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babelify": "^8.0.0",
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap4-plus-jquery": "^4.0.0-alpha.2.4",
    "browserify": "^14.5.0",
    "browserify-css": "^0.13.1",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "less": "^2.7.3",
    "lessify": "^1.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-websocket": "^1.2.0",
    "reactify": "^1.1.1",
    "scssify": "^3.0.1",
    "svg-browserify": "0.0.2",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "start": "npm run frontend & npm run backend",
    "frontend": "watchify -o public/bundle.js -v -d src/index.js",
    "backend": "cd ../backend; go run *.go"
  },
  "browserify": {
    "transform": [
      [
        "scssify",
        {
          "sass": {
            "includePaths": [
              "node_modules"
            ]
          }
        }
      ],
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1"
  }
}
